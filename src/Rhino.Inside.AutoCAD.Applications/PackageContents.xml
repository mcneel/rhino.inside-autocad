				<Command Local="RHINO_INSIDE_CONVERT_BREP" Global="RHINO_INSIDE_CONVERT_BREP" />
			</Commands>
		</ComponentEntry>


		<ComponentEntry AppName="Rhino.Inside.AutoCAD"
						ModuleName="./1.0.14/Win64/NET48/Rhino.Inside.AutoCAD.Interop.dll"
						AppDescription="Interop Dependency"
						AppType="Managed"/>

		<ComponentEntry AppName="Rhino.Inside.AutoCAD"
						ModuleName="./1.0.14/Win64/NET48/Rhino.Inside.AutoCAD.Services.dll"
						AppDescription="Services Dependency"
						AppType="Managed"/>

		<ComponentEntry AppName="Rhino.Inside.AutoCAD"
						ModuleName="./1.0.14/Win64/NET48/Rhino.Inside.AutoCAD.UI.Resources.dll"
